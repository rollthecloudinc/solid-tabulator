{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "defaultProject": "shell",
  "projects": {
    "shell": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/shell",
      "sourceRoot": "projects/shell/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-architects/native-federation:build",
          "options": {
            "outputPath": "dist/shell",
            "index": "projects/shell/src/index.html",
            "main": "projects/shell/src/main.ts",
            "polyfills": "projects/shell/src/polyfills.ts",
            "tsConfig": "projects/shell/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/shell/src/favicon.ico",
              "projects/shell/src/assets"
            ],
            "styles": [
              "projects/shell/src/styles.css"
            ],
            "scripts": [],
            "federationConfig": "projects/shell/federation.config.js"
          },
          "configurations": {
            "development": {
              "optimization": false,
              "sourceMap": true,
              "buildOptimizer": false
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/shell/src/environments/environment.ts",
                  "with": "projects/shell/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "buildTarget": "shell:build",
            "publicHost": "http://localhost:5000",
            "port": 5000
          },
          "configurations": {
            "development": {
              "buildTarget": "shell:build:development"
            },
            "production": {
              "buildTarget": "shell:build:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "shell:build"
          }
        },
        "test": {
          "builder": "@angular-builders/custom-webpack:karma",
          "options": {
            "main": "projects/shell/src/test.ts",
            "polyfills": "projects/shell/src/polyfills.ts",
            "tsConfig": "projects/shell/tsconfig.spec.json",
            "karmaConfig": "projects/shell/karma.conf.js",
            "assets": [
              "projects/shell/src/favicon.ico",
              "projects/shell/src/assets"
            ],
            "styles": [
              "projects/shell/src/styles.css"
            ],
            "scripts": []
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/shell/e2e/protractor.conf.js",
            "devServerTarget": "shell:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "shell:serve:production"
            }
          }
        }
      }
    },
    "plugin": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/plugin",
      "sourceRoot": "projects/plugin/src",
      "prefix": "app",
      "architect": {
        "esbuild": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/plugin",
            "index": "projects/plugin/src/index.html",
            "polyfills": ["projects/plugin/src/polyfills.ts"],
            "tsConfig": "projects/plugin/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/plugin/src/favicon.ico",
              "projects/plugin/src/assets"
            ],
            "styles": [
              "projects/plugin/src/styles.css"
            ],
            "scripts": [],
            "define": {
              "process": "{\"env\": {\"NODE_ENV\": \"development\"}}", 
              "process.env.NODE_ENV": "\"development\""
            }
          },
          "configurations": {
            "development": {
              "optimization": false,
              "sourceMap": true,
              "outputHashing": "all",
              "extractLicenses": true
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/plugin/src/environments/environment.ts",
                  "with": "projects/plugin/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true
            }
          }
        },
        "serve": {
          "builder": "@angular-architects/native-federation:build",
          "options": {
            "target": "plugin:serve-original:development",
            "tsConfig": "projects/plugin/tsconfig.app.json",
            "rebuildDelay": 0,
            "dev": true,
            "port": 3000
          },
          "configurations": {
            "development": {
              "target": "plugin:serve-original:development"
            },
            "production": {
              "dev": false,
              "target": "plugin:serve-original:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "serve-original": {
          "builder": "@angular-devkit/build-angular:dev-server", 
          "options": {
            "port": 3000,
            "buildTarget": "plugin:esbuild:development"
          },
          "configurations": {
            "production": {
              "buildTarget": "plugin:esbuild:production"
            },
            "development": {
              "buildTarget": "plugin:esbuild:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "build": {
          "builder": "@angular-architects/native-federation:build",
          "options": {
            "target": "plugin:esbuild:development",
            "tsConfig": "projects/plugin/tsconfig.app.json",
            "rebuildDelay": 0,
            "dev": true,
            "port": 3000
          },
          "configurations": {
            "development": {
              "target": "plugin:esbuild:development"
            },
            "production": {
              "dev": false,
              "target": "plugin:esbuild:production"
            }
          },
          "defaultConfiguration": "production"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "plugin:build"
          }
        },
        "test": {
          "builder": "@angular-builders/custom-webpack:karma",
          "options": {
            "main": "projects/plugin/src/test.ts",
            "polyfills": "projects/plugin/src/polyfills.ts",
            "tsConfig": "projects/plugin/tsconfig.spec.json",
            "karmaConfig": "projects/plugin/karma.conf.js",
            "assets": [
              "projects/plugin/src/favicon.ico",
              "projects/plugin/src/assets"
            ],
            "styles": [
              "projects/plugin/src/styles.css"
            ],
            "scripts": []
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/plugin/e2e/protractor.conf.js",
            "devServerTarget": "plugin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "plugin:serve:production"
            }
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  },
  "cli": {
    "analytics": false
  }
}